{
  "name": "juneo-blocks",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create-local": "graph create --node http://192.168.10.10:8020/ blocks",
    "deploy-local": "graph deploy --node http://127.0.0.1:8020/ --ipfs http://192.168.10.15:5001 blocks",
    "remove-local": "graph remove --node http://127.0.0.1:8020/ blocks",
    "prepare:socotra": "mustache config/juneo-socotra.json subgraph.template.yaml > subgraph.yaml && npm run codegen",
    "deploy:socotra": "npm run prepare:socotra && graph deploy TODO blocks"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.68.0",
    "@graphprotocol/graph-ts": "^0.32.0",
    "mustache": "^4.2.0"
  }
}
